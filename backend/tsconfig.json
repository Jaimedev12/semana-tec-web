{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "outDir": "lib",
      "sourceMap": true,
      "strict": true,
      "target": "ES2022",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "esModuleInterop": true
    },
    "compileOnSave": true,
    "include": [
      "functions/**/*",
    ]
  }
  